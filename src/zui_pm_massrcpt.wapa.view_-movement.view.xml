<mvc:View controllerName="com.toray.massreceipt.controller.Movement"&nbsp;                                                                                                                                                                                     
    displayBlock="true"                                                                                                                                                                                                                                        
    xmlns:mvc="sap.ui.core.mvc"&nbsp;                                                                                                                                                                                                                          
    xmlns:f="sap.f"                                                                                                                                                                                                                                            
    xmlns="sap.m"&nbsp;                                                                                                                                                                                                                                        
    xmlns:smartTable="sap.ui.comp.smarttable">                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
    <f:DynamicPage preserveHeaderStateOnScroll="true" showFooter="true" class="sapUiNoContentPadding">                                                                                                                                                         
        <f:title>                                                                                                                                                                                                                                              
            <f:DynamicPageTitle>                                                                                                                                                                                                                               
                <f:heading>                                                                                                                                                                                                                                    
                    <Title text="{i18n>title}" />                                                                                                                                                                                                              
                </f:heading>                                                                                                                                                                                                                                   
            </f:DynamicPageTitle>                                                                                                                                                                                                                              
        </f:title>                                                                                                                                                                                                                                             
        <f:header>                                                                                                                                                                                                                                             
        </f:header>                                                                                                                                                                                                                                            
        <f:content>                                                                                                                                                                                                                                            
            <Table id="MovementsTable"                                                                                                                                                                                                                         
                   mode="SingleSelectMaster"                                                                                                                                                                                                                   
                   autoPopinMode="true">                                                                                                                                                                                                                       
                <columns>                                                                                                                                                                                                                                      
                    <Column width="30px"/>                                                                                                                                                                                                                     
                    <Column width="100px">                                                                                                                                                                                                                     
                        <Text text="{i18n>PurchaseOrder}" />                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column width="60px">                                                                                                                                                                                                                      
                        <Text text="{i18n>Item}" />                                                                                                                                                                                                            
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>Supplier}" />                                                                                                                                                                                                        
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>Material}" />                                                                                                                                                                                                        
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>StillToBeDeliveredQuantity}" />                                                                                                                                                                                      
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>Quantity}" />                                                                                                                                                                                                        
                    </Column>                                                                                                                                                                                                                                  
                    <Column width="150px">                                                                                                                                                                                                                     
                        <Text text="{i18n>PostingDate}" />                                                                                                                                                                                                     
                    </Column>                                                                                                                                                                                                                                  
                    <Column width="150px">                                                                                                                                                                                                                     
                        <Text text="{i18n>DocumentDate}" />                                                                                                                                                                                                    
                    </Column>                                                                                                                                                                                                                                  
                    <!-- <Column>                                                                                                                                                                                                                              
                        <Text text="{i18n>Plant}" />                                                                                                                                                                                                           
                    </Column> -->                                                                                                                                                                                                                              
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>StorageLocation}" />                                                                                                                                                                                                 
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>Batch}" />                                                                                                                                                                                                           
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>ValuationType}" />                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>SerialNumbers}" />                                                                                                                                                                                                   
                    </Column>                                                                                                                                                                                                                                  
                    <Column>                                                                                                                                                                                                                                   
                        <Text text="{i18n>MaterialDocument}" />                                                                                                                                                                                                
                    </Column>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                          
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
                </items>                                                                                                                                                                                                                                       
            </Table>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                           
        </f:content>                                                                                                                                                                                                                                           
        <f:footer>                                                                                                                                                                                                                                             
			<OverflowToolbar>                                                                                                                                                                                                                                           
                <Button id="messagePopoverBtn" icon="sap-icon://message-popup" press="handleMessagePopoverPress" type="{= (${message>/}.length > 0 ? 'Emphasized':'Default')}" />                                                                              
			    <ToolbarSpacer/>                                                                                                                                                                                                                                        
                <Button text="{i18n>button.cancel}" type="Back" press="onCancel" />                                                                                                                                                                            
				<Button text="{i18n>button.executeReceipt}" type="Accept" press="onPerformGoodReceipt" />                                                                                                                                                                  
			</OverflowToolbar>                                                                                                                                                                                                                                          
        </f:footer>                                                                                                                                                                                                                                            
    </f:DynamicPage>&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               