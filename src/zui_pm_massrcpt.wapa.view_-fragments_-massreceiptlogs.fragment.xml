<core:FragmentDefinition&nbsp;                                                                                                                                                                                                                                 
    xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"                                                                                                                                                                                                          
    xmlns:smarttable="sap.ui.comp.smarttable"                                                                                                                                                                                                                  
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    xmlns="sap.m">                                                                                                                                                                                                                                             
&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                                       
    <Dialog title="{i18n>MassReceiptLogsTitle}" stretch="true">                                                                                                                                                                                                
        <Page showHeader="false" backgroundDesign="Transparent">                                                                                                                                                                                               
            <content>                                                                                                                                                                                                                                          
                <smarttable:SmartTable                                                                                                                                                                                                                         
                id="historySmartTable"                                                                                                                                                                                                                         
                smartFilterId="historySmartFilterBar"                                                                                                                                                                                                          
                entitySet="ZC_PM_MassReceiptLog"&nbsp;                                                                                                                                                                                                         
                enableAutoBinding="true">                                                                                                                                                                                                                      
                    <smarttable:layoutData>                                                                                                                                                                                                                    
                        <FlexItemData growFactor="1" baseSize="100%"/>                                                                                                                                                                                         
                    </smarttable:layoutData>                                                                                                                                                                                                                   
                    <Table>                                                                                                                                                                                                                                    
                    </Table>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                           
                </smarttable:SmartTable>                                                                                                                                                                                                                       
            </content>                                                                                                                                                                                                                                         
            <footer>                                                                                                                                                                                                                                           
                <OverflowToolbar>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                              
                    <ToolbarSpacer/>                                                                                                                                                                                                                           
                    <Button text="{i18n>button.close}" type="Emphasized" press="onCloseHistory"/>                                                                                                                                                              
			    </OverflowToolbar>&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                    
            </footer>                                                                                                                                                                                                                                          
        </Page>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                
    </Dialog>                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               