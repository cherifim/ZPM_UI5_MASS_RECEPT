/**                                                                                                                                                                                                                                                            
 * eslint-disable @sap/ui5-jsdocs/no-jsdoc                                                                                                                                                                                                                     
 */                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
sap.ui.define([                                                                                                                                                                                                                                                
        "sap/ui/core/UIComponent",                                                                                                                                                                                                                             
        "sap/ui/Device",                                                                                                                                                                                                                                       
        "com/toray/massreceipt/model/models"                                                                                                                                                                                                                   
    ],                                                                                                                                                                                                                                                         
    function (UIComponent, Device, models) {                                                                                                                                                                                                                   
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return UIComponent.extend("com.toray.massreceipt.Component", {                                                                                                                                                                                         
            metadata: {                                                                                                                                                                                                                                        
                manifest: "json",                                                                                                                                                                                                                              
                config: { fullWidth: true }                                                                                                                                                                                                                    
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            /**                                                                                                                                                                                                                                                
             * The component is initialized by UI5 automatically during the startup of the app and calls the init method once.                                                                                                                                 
             * @public                                                                                                                                                                                                                                         
             * @override                                                                                                                                                                                                                                       
             */                                                                                                                                                                                                                                                
            init: function () {                                                                                                                                                                                                                                
                // call the base component's init function                                                                                                                                                                                                     
                UIComponent.prototype.init.apply(this, arguments);                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
                // enable routing                                                                                                                                                                                                                              
                this.getRouter().initialize();                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
                this._bSelectionChanged = true;                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
                // Cr√©ation du groupe                                                                                                                                                                                                                          
                this.getModel().setChangeGroups({                                                                                                                                                                                                              
                    "GoodMovement": {                                                                                                                                                                                                                          
                        groupId: "good_receipt",                                                                                                                                                                                                               
                        single: true                                                                                                                                                                                                                           
                    }                                                                                                                                                                                                                                          
                });                                                                                                                                                                                                                                            
                this.getModel().setDeferredGroups(this.getModel().getDeferredGroups().concat(["good_receipt"]));                                                                                                                                               
                                                                                                                                                                                                                                                               
                // set the device model                                                                                                                                                                                                                        
                this.setModel(models.createDeviceModel(), "device");                                                                                                                                                                                           
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            setSelectedItemsContexts: function (aContexts) {                                                                                                                                                                                                   
                this._selectedPOItemsContexts = aContexts;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                     
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            getSelectedItemsContexts: function () {&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                            
                return this._selectedPOItemsContexts;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                          
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            clearSelectedItemsContexts: function () {                                                                                                                                                                                                          
                this._selectedPOItemsContexts = [];                                                                                                                                                                                                            
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            /**                                                                                                                                                                                                                                                
             * Set whether the selection has changed.                                                                                                                                                                                                          
             *                                                                                                                                                                                                                                                 
             * @param {boolean} bSelectionChanged - The new value for selection changed                                                                                                                                                                        
             */                                                                                                                                                                                                                                                
            setSelectionChanged: function (bSelectionChanged) {                                                                                                                                                                                                
                this._bSelectionChanged = bSelectionChanged;                                                                                                                                                                                                   
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            /**                                                                                                                                                                                                                                                
             * Get the selection changed.                                                                                                                                                                                                                      
             *                                                                                                                                                                                                                                                 
             * @return {undefined} No return value.                                                                                                                                                                                                            
             */                                                                                                                                                                                                                                                
            getSelectionChanged: function () {                                                                                                                                                                                                                 
                return this._bSelectionChanged;                                                                                                                                                                                                                
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
        });                                                                                                                                                                                                                                                    
    }                                                                                                                                                                                                                                                          
);                                                                                                                                                                                                                                                             