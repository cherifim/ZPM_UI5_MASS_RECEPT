<core:FragmentDefinition&nbsp;                                                                                                                                                                                                                                 
    xmlns:navpopover="sap.ui.comp.navpopover"&nbsp;                                                                                                                                                                                                            
    xmlns:smartfield="sap.ui.comp.smartfield"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                    
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    xmlns="sap.m">                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
    <ColumnListItem class="movementRowStyle" highlight="{= (${Created}?'Success':'None') }">                                                                                                                                                                   
        <!-- <customData>                                                                                                                                                                                                                                      
            <core:CustomData&nbsp;                                                                                                                                                                                                                             
                key="matDocCreated"                                                                                                                                                                                                                            
                value="{=''+${Created}}"                                                                                                                                                                                                                       
                writeToDom="true"                                                                                                                                                                                                                              
            />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                         
        </customData> -->                                                                                                                                                                                                                                      
        <cells>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                          
            <core:Icon press="onRemoveMovement" src="sap-icon://delete"/>&nbsp;&nbsp;                                                                                                                                                                          
            <smartfield:SmartField value="{PurchasingDocumentNumber}"/>                                                                                                                                                                                        
            <smartfield:SmartField value="{ItemNumber}"/>                                                                                                                                                                                                      
            <ObjectIdentifier title="{POItem>SupplierName}" text="{POItem>Supplier}"/>                                                                                                                                                                         
            <ObjectIdentifier title="{POItem>MaterialName}" text="{POItem>Material}"/>                                                                                                                                                                         
            <ObjectIdentifier title="{POItem>StillToBeDeliveredQuantity}"/>                                                                                                                                                                                    
            <smartfield:SmartField value="{Quantity}"/>                                                                                                                                                                                                        
            <smartfield:SmartField value="{PostingDate}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                             
            <smartfield:SmartField value="{DocumentDate}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                            
            <!-- <smartfield:SmartField value="{Plant}"/> -->                                                                                                                                                                                                  
            <smartfield:SmartField value="{StorageLocation}"/>                                                                                                                                                                                                 
            <smartfield:SmartField value="{BatchNumber}" enabled="{POItem>BatchManagement}"/>                                                                                                                                                                  
            <smartfield:SmartField value="{ValuationType}" enabled="{POItem>ValuationTypeRequired}"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                  
            <!-- <ComboBox selectedKey="{ValuationType}" items="{/ValuationTypeSH}" showSecondaryValues="true" enabled="{POItem>ValuationTypeRequired}">                                                                                                       
                <items>                                                                                                                                                                                                                                        
                    <core:ListItem key="{ValuationType}" text="{ValuationType}"/>                                                                                                                                                                              
                </items>                                                                                                                                                                                                                                       
            </ComboBox> -->                                                                                                                                                                                                                                    
            <MultiInput showValueHelp="false" showSuggestion="false" maxLength="8" visible="{POItem>SerialNumberRequired}" editable="{= !${Created} }"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;+
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                             
            <!-- <Text text="{MaterialDocument} - {MaterialDocumentYear}" class="bold green" visible="{Created}"/>  -->                                                                                                                                        
            <Link text="{MaterialDocument} - {MaterialDocumentYear}"                                                                                                                                                                                           
                  press="onMatDoccumentPress"                                                                                                                                                                                                                  
                  visible="{Created}"/>                                                                                                                                                                                                                        
            <!-- <navpopover:SmartLink&nbsp;                                                                                                                                                                                                                   
                semanticObject="MaterialMovement"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                              
                beforeNavigationCallback=".onBeforeNavigationToMatDocument"                                                                                                                                                                                    
                text="{MaterialDocument} - {MaterialDocumentYear}"&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                       
                visible="{Created}"/> -->&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                              
        </cells>                                                                                                                                                                                                                                               
    </ColumnListItem>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
</core:FragmentDefinition>&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                             